<AnchorPane prefHeight="366.0" prefWidth="300.0" style="-fx-background-color: #2D3442;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainControllerSignUp">
   <children>
