         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="RegTxtName" layoutX="45.0" layoutY="109.0" prefHeight="32.0" prefWidth="188.0" promptText="Name" style="-fx-background-color: #a0a2ab;" />
      <TextField fx:id="RegTxtUserName" layoutX="46.0" layoutY="160.0" prefHeight="32.0" prefWidth="188.0" promptText="User Name" style="-fx-background-color: #a0a2ab;" />
